  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "preview": "vite preview",
    "prepare": "npm run lint && npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
